{
    "manifest_version": 2,
    "name": "Découvertes",
    "version": "1.0",
    "description": "Découvertes: Helps you in forgetting websites. Forces you to discover new websites.",
    "icons": {
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "decouvertes.js"
            ]
        }
    ]
}